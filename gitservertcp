import socket
import ujson

s=socket.socket()
print("socket created")
port= 3000
s.bind(('',port) )
print("socket binded to the port 3000")
s.listen()

while True:
    c, addr =s.accept()
    print("got connection from {}".format(addr))
    data =ujson.loads(c.recv(1024).decode('utf-8'))
    print("received {}".format(data))
    c.send(ujson.dumps('12').encode('utf-8'))
    c.close()



