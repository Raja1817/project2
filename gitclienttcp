import socket
import ujson

s=socket.socket()
port= 3000

s.connect( ('192.168.1.204' , port) )
print("connecting to {}:{}".format('192.168.1.204' , port))
s.send(ujson.dumps('12').encode('utf-8'))
d = ujson.loads(s.recv(1024).decode('utf-8'))  
print("received {} ".format(d))

print("Thank you")

Print("gitclient")


s.close()
